if(mensaje[0]==="!duelo"){
      if(!dueloActivo){
        dueloActivo=true;;
        let mentioned = msg.mentions.members.first();
        let autor = msg.author.id;
        let autorSp = userSp(autor);
        let valor;
        if(mensaje[2]){
          if(!isNaN(mensaje[2])){
            valor = Math.abs(mensaje[2]);
          }else if(mensaje[2]==="all" && autorSp>0){
            valor = 0;
          }else{
            valor = 1;
          }
        }else{
          valor = 1;
        }
        let embed,embed2;
        if(autorSp>=valor){
          let rand = Math.random();
          if(mentioned){
            let mentionedSp = userSp(mentioned);
            if(valor==0 && mentionedSp>0){
              valor = autorSp>mentionedSp ? mentionedSp : autorSp;
            }
            if(valor>0 && mentionedSp>=valor){
              modifyUser(autor,false,-valor);
              const filter = (reaction, user) => {return (user.id == mentioned) && (reaction.emoji.name === "ğŸ‘" || reaction.emoji.name === "ğŸ‘")};
              embed = new Discord.MessageEmbed()
                .setColor("#00BFFF")
                .setTitle("Â¡"+userName(autor)+" desafÃ­a a "+userName(mentioned)+"!")
