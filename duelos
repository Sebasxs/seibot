                    dueloActivo=false;
                  }).catch(collected => {
                    embed2 = new Discord.MessageEmbed()
                      //.setColor("#1E90FF")
                      .setTitle("Â¡"+userName(mentioned)+" se cagÃ³!")
                      .setThumbnail("https://cdn.discordapp.com/attachments/732342721217429614/737670666358095912/loser.png")
