                    m.edit(embed2);
                    dueloActivo=false;
                  })
              })            
            }else{
              msg.channel.send("<@"+mentioned+"> no dispone de suficientes SP para aceptar el duelo.")
              dueloActivo=false;
            }
          }else{
            //duelo libre
            if(mensaje[1]){
              if(!isNaN(mensaje[1])){
                valor = Math.abs(mensaje[1]);
              }else if(mensaje[1]==="all"){
                valor = 0;
              }else{
                valor = 1;
              }
            }else{
              valor = 1;
            }
            const filter = (reaction, user) => {return  userSp(user.id)>=valor && userSp(user.id)>0 && reaction.emoji.name === "ğŸ‘" && user.id != autor};
            embed = new Discord.MessageEmbed()
              .setColor("#00BFFF")
              .setTitle("Â¡"+userName(autor)+" desafÃ­a a TODOS!")
